			</an:p>
			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#IDZWIHGFD2103KIW3CIOTUEGIOSK3T0M44NS0UANCJ1JQ4CSED3DUH">RUTA</an:docProponent>
