			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#ID5PZ4JYI1VP2LO1ZDQHLY35ZF0CP14QYK5ESA31NORE5RPNXPGAZ">DI GIORGI</an:docProponent>
