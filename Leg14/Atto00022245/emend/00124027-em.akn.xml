			<an:amendmentContent>
				<an:p>
					<an:i>Dopo l'</an:i>
					articolo 18, 
					<an:i>inserire il seguente:</an:i>
				</an:p>
				<an:p align="center">
					<an:i/>
					«Art. 18-
					<an:i>bis.</an:i>
				</an:p>
