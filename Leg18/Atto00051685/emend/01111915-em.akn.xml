				<an:affectedDocument href="#ar1">Ddl 1248</an:affectedDocument>
			</an:p>
			<an:p>
				<an:docNumber>Emendamento n. 1.17 (testo corretto)</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E5">
					<an:span style="font-variant:small-caps;">Le</an:span>
					 
					<an:span style="font-variant:small-caps;">Commissioni</an:span>
					 
					<an:span style="font-variant:small-caps;">riunite</an:span>
				</an:docProponent>
			</an:p>
