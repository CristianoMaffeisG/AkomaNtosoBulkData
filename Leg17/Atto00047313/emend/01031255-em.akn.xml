			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#IDLCOKJM1L1ZOTES2RANPJ3OJYAIMIAQGCBGYNB4MFIM4OBCS2BOSB">GIOVANARDI</an:docProponent>
