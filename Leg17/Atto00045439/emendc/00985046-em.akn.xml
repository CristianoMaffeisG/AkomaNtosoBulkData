			</an:p>
			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#IDZRZGUXINJVFXDTMFUIFBA0VI5JBIR5IUEUQBE3OQMUJN5EUZRF0">RUTA</an:docProponent>
