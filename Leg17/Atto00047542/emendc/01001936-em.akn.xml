							<an:td>
								<an:p>
									<an:b>Termine</an:b>
								</an:p>
							</an:td>
							<an:td>
								<an:p>
									<an:b>Fonte Normativa</an:b>
								</an:p>
							</an:td>
						</an:tr>
						<an:tr>
							<an:td>
								<an:p>31 dicembre 2013</an:p>
							</an:td>
							<an:td>
								<an:p>articolo 1, comma 482, della legge del 27 dicembre 2013, n.  147, nei limiti di spesa</an:p>
							</an:td>
