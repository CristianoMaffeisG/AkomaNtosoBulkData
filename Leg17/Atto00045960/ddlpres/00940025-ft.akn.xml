				d'iniziativa dei senatori 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e15">COTTI</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e17">BERTOROTTA</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e19">BUCCARELLA</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e21">CAPPELLETTI</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e23">FUCKSIA</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e26">LUCIDI</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e28">MARTON</an:docProponent>
				, 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e30">MONTEVECCHI</an:docProponent>
				 e 
				<an:docProponent as="#d1972e15a1327" refersTo="#d1972e32">SANTANGELO</an:docProponent>
			</an:p>
			<an:p> COMUNICATO ALLA PRESIDENZA IL 7 AGOSTO 2015 </an:p>
