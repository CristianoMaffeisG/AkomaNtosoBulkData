			<an:p>
				<an:docTitle>
					Modifica alla legge 20 dicembre 2012, n. 238, per il sostegno e la valorizzazione del 
					<an:i>festival</an:i>
					 Umbria Jazz
				</an:docTitle>
			</an:p>
			<an:p>
				d’iniziativa dei deputati 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaab1">SERENI</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaab3">ASCANI</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaab5">CIPRINI</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaab7">GALGANO</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaab9">GALLINELLA</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaac11">GIULIETTI</an:docProponent>
				, 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaac13">LAFFRANCO</an:docProponent>
				 e 
				<an:docProponent as="#w1ab1ab1b2aaab1a" refersTo="#w1ab1ab1b2aaac15">VERINI</an:docProponent>
			</an:p>
