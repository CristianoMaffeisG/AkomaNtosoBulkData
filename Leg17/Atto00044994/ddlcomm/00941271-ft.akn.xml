		<an:coverPage>
			<an:p>SENATO DELLA REPUBBLICA</an:p>
			<an:p>
				<an:docType>
					
                
					<an:span style="text-align:center;">
						<an:b>
							TESTO PROPOSTO DALLA 13
							<an:sup>a</an:sup>
							 COMMISSIONE
                        PERMANENTE
						</an:b>
						<an:eol/>
						(TERRITORIO, AMBIENTE, BENI AMBIENTALI)
					</an:span>
					<an:eol/>
					
            
				</an:docType>
			</an:p>
			<an:p>
				<an:docNumber/>
			</an:p>
			<an:p>
				<an:docIntroducer>
					
                
					<an:i>Relatore</an:i>
					
                
					<an:span style="font-variant:small-caps;">Vaccari</an:span>
					
            
				</an:docIntroducer>
			</an:p>
			<an:container id="w1ab1b1b3c15" name="Sezione">
				<an:p>
					PER IL 
					<an:eol/>
					DISEGNO DI LEGGE
				</an:p>
				<an:container id="w1ab1b1b3c15b3" name="Atto">
					<an:p>
						<an:docNumber/>
					</an:p>
					<an:p>
						<an:docTitle>
							Disposizioni in materia ambientale per promuovere misure di
                            
							<an:i>green economy</an:i>
							 e per il contenimento dell'uso eccessivo di
                        risorse naturali
						</an:docTitle>
					</an:p>
					
                            
                                
					<an:p>
						presentato dal 
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1b1a" refersTo="#">Ministro dell'ambiente e della tutela del territorio e del mare ()</an:docProponent>
						di concerto con il 
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1b3a" refersTo="#">Ministro dello sviluppo economico ()</an:docProponent>
						con il
                                        
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1b5a" refersTo="#">Ministro delle infrastrutture e dei trasporti ()</an:docProponent>
						con il 
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1b7a" refersTo="#">Ministro dell'economia e delle finanze ()</an:docProponent>
						con il
                                        
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1b9a" refersTo="#">Ministro per gli affari regionali e le autonomie ()</an:docProponent>
						con il 
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1c11a" refersTo="#">Ministro della salute ()</an:docProponent>
						e
                                    con il 
						<an:docProponent as="#w1ab1b1b3c15b3b3b1b1b1c13a" refersTo="#">Ministro per la pubblica amministrazione e la semplificazione ()</an:docProponent>
					</an:p>
					
                                
					<an:p>
						<an:i>(</an:i>
						V. 
						<an:i>Stampato Camera</an:i>
						
                                    
						<an:b>n. 2093</an:b>
						<an:i>)</an:i>
					</an:p>
					
                                
					<an:p>approvato dalla Camera dei
                                    deputati il 13 novembre 2014</an:p>
					
                                
					<an:p>
						
                                    
						<an:i>
							Trasmesso dal Presidente della Camera dei deputati alla
                                            Presidenza
							<an:eol/>
							il 17 novembre 2014
						</an:i>
						
                                
					</an:p>
					
                            
                        
