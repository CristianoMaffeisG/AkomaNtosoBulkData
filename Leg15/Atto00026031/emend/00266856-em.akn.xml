		<an:amendmentBody>
			<an:amendmentContent>
				<an:p>
					<an:i>Al comma 15, apportare le seguenti modifiche:</an:i>
				</an:p>
				<an:p align="justify">
					            
					<an:i>a) sostituire le parole:</an:i>
					 «2 per cento» 
					<an:i>con le seguenti:</an:i>
					 «1,3 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>b) sostituire le parole:</an:i>
					 «6 per cento» 
					<an:i>con le seguenti:</an:i>
					 «5,2 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>c) sostituire le parole:</an:i>
					 «1,50 per cento» 
					<an:i>con le seguenti:</an:i>
					 «1 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>d) sostituire le parole:</an:i>
					 «4,75 per cento» 
					<an:i>con le seguenti:</an:i>
					 «4 per cento»;
				</an:p>
				<an:p align="justify">
					<an:i>        Conseguentemente, all'articolo 22, comma 1, sostituire le parole:</an:i>
					 «10 per cento» 
					<an:i>con le seguenti:</an:i>
					 «15 per cento».
				</an:p>
				<an:p align="justify"/>
			</an:amendmentContent>
